cc_defaults {
    name: "NativeSFDemo_defaults",

    cflags: [
        "-Wall",
        "-Werror",
        "-Wunused",
        "-Wunreachable-code",
    ],
    shared_libs: [
        "libbase",
        "libbinder",
        "libcutils",
        "liblog",
        "libutils",
        "libui",
        "libgui",
        "libEGL",
        "libGLESv1_CM",
        "libnativewindow",
        "libskia",
    ],
}

// NativeSFDemo executable
// =========================================================

cc_binary {
    name: "NativeSFDemo",
    defaults: ["NativeSFDemo_defaults"],
    srcs: [
        "NativeSurfaceWrapper.cpp",
        "main_NativeSFDemo.cpp",
    ],

    cflags: [
        "-Wno-deprecated-declarations",
    ],
}

cc_defaults {
    name: "demo_defaults",

    cflags: [
        "-Wall",
        "-Werror",
        "-Wunused",
        "-Wunreachable-code",
    ],

    shared_libs: [
        "libbase",
        "libbinder",
        "libcutils",
        "liblog",
        "libutils",
        "libui",
        "libgui",
        "libEGL",
        "libGLESv1_CM",
    ],
}

cc_binary {
    name: "DispalyDemo",
    defaults: ["demo_defaults"],
    srcs: [
        "main.cpp",
    ],

    cflags: [
        "-Wno-deprecated-declarations",
        "-Wno-unused-parameter"
    ],
}

